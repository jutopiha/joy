<body>
  <header class="sign_up_logo">
    <a href="../views/main.html#"><img src="../public/images/MainLogo3.png" class="logo"alt="" style="height:40px;"></a>
  </header>
  <div class="sign_up_container">
    <div class="sign_up_Box">
      <div class="sign_up">
        <form action="/sign_up" method="post">
          <div>
            <label>Username:</label><br>
            <input type="text" name="username" />
          </div>
          <div>
            <label>Password:</label><br>
            <input type="password" name="password" />
          </div>
          <div>
            <label>DisplayName:</label><br>
            <input type="text" name="displayName" />
          </div>
          <div>
            <a href="/login"><button class="btn-sign-up" type="submit">Sign Up</button></a>
            <br>
            <a href="/login"><button class="btn-login" >Login</button></a>
          </div>
        </form>
      </div>
    </div>
  </div>
